    "N = int(input())\n",
    "s = str(input())\n",
    "\n",
    "r = 0\n",
    "b = 0\n",
    "for i in range(N):\n",
    "    if s[i] == 'R':\n",
    "        r += 1\n",
    "    else:\n",
    "        b += 1\n",
    "\n",
    "if r > b:\n",
    "    print('Yes')\n",
    "else:\n",
    "    print('No')"
